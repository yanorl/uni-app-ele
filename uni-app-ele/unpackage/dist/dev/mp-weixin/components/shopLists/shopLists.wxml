<view class="shop-list"><view data-event-opts="{{[['tap',[['selectShop',['$0'],['shopList.id']]]]]}}" class="shop-list-info" bindtap="__e"><view class="shop-list-logo"><image src="{{shopList.image_path}}"></image></view><view class="shop-list-content"><view class="shop-list-name"><block wx:if="{{shopList.is_premium}}"><view class="_i">品牌</view></block><text>{{shopList.name}}</text></view><view class="shop-list-rate"><view class="rate-wrap"><rating vue-id="317c6d82-1" rating="{{shopList.rating}}" bind:__l="__l"></rating><text class="rate-num">{{shopList.rating}}</text><text>{{"月售"+shopList.recent_order_num+"单"}}</text></view><block wx:if="{{shopList.delivery_mode}}"><view class="delivery"><text class="icon-hollow">{{shopList.delivery_mode.text}}</text></view></block></view><view class="shop-delivery"><view><text>{{"¥"+shopList.float_minimum_order_amount+"起送"}}</text>|<text>{{"配送费¥"+shopList.float_delivery_fee}}</text></view><view class="shop-delivery-distance"><text>{{$root.g0+"km"}}</text>|<text>{{shopList.order_lead_time+"分钟"}}</text></view></view></view></view></view>