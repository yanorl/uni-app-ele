<view><view class="goods-list"><block wx:if="{{goodsList.length==0}}"><view class="empty">购物车空空如也~</view></block><block wx:for="{{goodsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="row"><view data-event-opts="{{[['tap',[['handleSingleDelete',['$0'],[[['goodsList','',index]]]]]]]}}" class="menu" bindtap="__e"><view class="icon iconfont"></view></view><view data-event-opts="{{[['touchstart',[['handleTouchStart',[index,'$event']]]],['touchmove',[['handleTouchMove',[index,'$event']]]],['touchend',[['handleTouchEnd',[index,'$event']]]]]}}" class="{{['production',theIndex==index?'open':oldIndex==index?'close':'']}}" bindtouchstart="__e" bindtouchmove="__e" bindtouchend="__e"><view data-event-opts="{{[['tap',[['handleCheckbox',['$0'],[[['goodsList','',index]]]]]]]}}" class="container" bindtap="__e"><view class="checkbox"><view class="{{[(item.selected)?'on':'']}}"></view></view></view><view data-event-opts="{{[['tap',[['handleGoodsInfo',['$0'],[[['goodsList','',index]]]]]]]}}" class="goods-info" bindtap="__e"><view class="img"><image src="{{item.food_image}}"></image></view><view class="info"><view class="title">{{item.food_name}}</view><view class="price-number"><view class="price">{{"￥"+item.food_price}}</view><view class="cart-control-wrapper"><cart-control vue-id="{{'63df024f-1-'+index}}" items="{{item}}" cartSum="{{cartSum}}" data-event-opts="{{[['^add',[['add',['$0','$event'],[[['goodsList','',index]]]]]],['^sub',[['sub',['$0','$event'],[[['goodsList','',index]]]]]]]}}" bind:add="__e" bind:sub="__e" bind:__l="__l"></cart-control></view></view></view></view></view></view></block></view><view class="footer" style="{{'bottom:'+(footerbottom)+';'}}"><view data-event-opts="{{[['tap',[['handleSelectedAll',['$event']]]]]}}" class="container" bindtap="__e"><view class="checkbox"><view class="{{[(isAllSelected)?'on':'']}}"></view></view><view class="text">全选</view></view><block wx:if="{{selectedList.length>0}}"><view data-event-opts="{{[['tap',[['handleMulDelete',['$event']]]]]}}" class="delBtn" bindtap="__e">删除</view></block><view class="settlement"><view class="sum">合计:<view class="money">{{"￥"+sumPrice}}</view></view><view data-event-opts="{{[['tap',[['handleConfirm',['$event']]]]]}}" class="btn" bindtap="__e">{{"结算"+selectedList.length}}</view></view></view></view>