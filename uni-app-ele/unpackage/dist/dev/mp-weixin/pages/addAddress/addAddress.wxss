.addAddress-wrap .addAddress-box .addAddress-content .form-content {
  padding: 0 30rpx;
  background-color: #ffffff;
  font-size: 28rpx;
}
.addAddress-wrap .addAddress-box .addAddress-content .form-content .form-group {
  padding: 30rpx 0;
  border-bottom: 1px solid #eee;
}
.addAddress-wrap .addAddress-box .addAddress-content .form-content .form-group label {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.addAddress-wrap .addAddress-box .addAddress-content .form-content .form-group label > text {
  -webkit-flex-basis: 100rpx;
          flex-basis: 100rpx;
  color: #333;
  font-weight: 700;
}
.addAddress-wrap .addAddress-box .addAddress-content .form-content .form-group label .input-wrap {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  font-size: 24rpx;
}
.addAddress-wrap .addAddress-box .addAddress-content .form-content .form-group label .input-wrap ._i {
  margin-left: 10rpx;
}
.addAddress-wrap .addAddress-box .addAddress-content .form-content .form-group label .input-wrap .which-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.addAddress-wrap .addAddress-box .addAddress-content .form-content .form-group label .input-wrap .which-box text {
  background: #fff;
  border: 1px solid #ddd;
  border-radius: 8rpx;
  font-size: 28rpx;
  color: #666;
  height: 60rpx;
  padding: 0 40rpx;
  line-height: 60rpx;
  margin-right: 20rpx;
  text-align: center;
  font-weight: normal;
}
.addAddress-wrap .addAddress-box .addAddress-content .form-content .form-group label .input-wrap .which-box text.checked {
  color: #2395ff !important;
  background: #eef7ff !important;
  border-color: rgba(35, 149, 255, 0.18) !important;
}
.addAddress-wrap .addAddress-box .addAddress-content .form-button-wrap {
  padding: 40rpx 30rpx;
}
.addAddress-wrap .addAddress-box .addAddress-content .form-button-wrap .form-button {
  background: #00d762;
  text-align: center;
  border-radius: 4rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  color: #fff;
  border: none;
  font-weight: 500;
}
.addAddress-wrap .address-search {
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 999;
}

